{
  "name":  "jwt auth",
  "description": "Json Web Token authenctication for laravel",
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "~5.0",
    "illuminate/http": "~5.0",
    "namshi/jose": "5.0.*",
    "nesbot/carbon": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "illuminate/auth":"5.0.*",
    "illuminate/database": "~5.0",
    "illuminate/console": "~5.0"
  }
  "autoload": {
    "psr-4": {
      "Tymon\\JWTAuth\\": "src"
      }
    },
  "autoload-dev": {
    "psr-4": {
      "Tymon\\JWTAuth\\Test\\": "tests"
      }
    }
}
